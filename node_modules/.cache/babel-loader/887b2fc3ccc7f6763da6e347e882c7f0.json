\n  </div>\n</template>\n<script>\n  export default {\n    name: \"App\",\n    data() {\n      return {\n        hide_menu: true,\n      };\n    },\n\n    mounted: async function () {\n      // Add class to body when we open any sidebar.\n      // eslint-disable-next-line\n      this.$root.$on(\"bv::collapse::state\", (sidebar_id, value) => {\n        let element = document.getElementById(sidebar_id);\n        if (element && ((element || {}).classList || []).contains(\"b-sidebar\")) {\n          if (value) document.body.classList.add(\"sidebar_opened\");\n          else document.body.classList.remove(\"sidebar_opened\");\n        }\n      });\n      // Add class to body when we open any modal.\n      let modal_id = [];\n      // eslint-disable-next-line no-unused-vars\n      this.$root.$on(\"bv::modal::show\", (event, id) => {\n        this.$nextTick(() => {\n          modal_id.push(id);\n          let tag = document.getElementsByTagName(\"html\")[0];\n          tag.classList.add(\"hide-body-scroll\");\n        });\n      });\n      // eslint-disable-next-line no-unused-vars\n      this.$root.$on(\"bv::modal::hide\", (event, id) => {\n        let index = modal_id.findIndex((row) => {\n          return row == id;\n        });\n        if (index >= 0) modal_id.splice(index, 1);\n        if (!modal_id.length) {\n          let tag = document.getElementsByTagName(\"html\")[0];\n          tag.classList.remove(\"hide-body-scroll\");\n        }\n      });\n    },\n    methods: {},\n  };\n</script>\n\n<style lang=\"scss\"></style>"]}]}